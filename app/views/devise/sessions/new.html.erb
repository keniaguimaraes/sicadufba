<br><br>
<section class="section">
  <div class="row">
    <div class="col s12 lg6 m6 offset-m3">
        <div class="row">
          <div class="col  s12 lg12 m12">
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
              <div class="input-field  s12 lg12 m12">
                 <i class="material-icons prefix">email</i> 
                <%= f.email_field :email, autofocus: true %>
                <%= f.label :email %>
              </div>
              <br>
              <div class="input-field  s12 lg12 m12">
                   <i class="material-icons prefix">lock</i> 
                <%= f.password_field :password, autocomplete: "off" %>
                <%= f.label :password %>
              </div>

              <div class="input-field  s12 lg12 m12">
                <%= f.submit "Entrar", class: 'btn waves-effect waves-light light-blue btn-small white-text' %>
              </div>

              <div class="input-field  s12 lg12 m12">
                <%= render "devise/shared/links" %>
              </div>
            <% end %>
          </div>
        </div>
      
    </div>
  </div>
</section>