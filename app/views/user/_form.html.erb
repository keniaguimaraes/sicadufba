<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
 <div class="row">
 <div class="col s12 m8 lg9 offset-m2">
          
  <div class="card-panel">

      <div class="row">
          <div class="col s12 m12 lg12"><b><%= f.label :professor_id, "E-mail" %></b></div> 
               <div class="col s12 m12 lg12"><%= f.text_field :email, id: :email, :readonly=>true%></div>
           </div> 

           <div class="row">
              <div class="col s12 m12 lg12"><b><%= f.label :username, "Username" %></b></div> 
              <div class="col s12 m12 lg12"><%= f.text_field :username, id: :username,:readonly=>true %></div>
           </div> 
           
           <div class="row">         
              <div class="col s12 m1 l1"><b><%= f.label :administrador, "Administrador?" %></b></div> 
              <div class="col s12 m1 lg1 offset-m2 offset-m2"><label> <%= f.check_box(:administrador, {}, true, false)%> <span> </span> </label> </div>
           </div>  
           
           <div class="row">         
               <divclass="col s12 m12 lg12"> 
                 <%= f.submit "OK", class: 'btn waves-effect waves-light light-blue' %> 
              </div>  
           </div>  
          
  </div> 
  </div> 
   </div> 
   
<% end %>
